{
  "name": "wai-agent-workflow",
  "main": "src/index.ts",
  "compatibility_date": "2025-03-07",
  "compatibility_flags": ["nodejs_compat"],
  "observability": { "enabled": true },
  "durable_objects": {
    "bindings": [
      { "name": "WAI_WorkflowAgent", "class_name": "WAI_WorkflowAgent" }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["WAI-WorkflowAgent"]
    },
    {
      "tag": "v2",
      "delete_class": ["WAI-WorkflowAgent"]
    },
    {
      "tag": "v3",
      "new_sqlite_classes": ["WAI_WorkflowAgent"]
    }
  ],
  "workflows": [
    { "name": "WAI_demo_workflow", "binding": "WAI_DEMO_WORKFLOW", "class_name": "WAI_DemoWorkflow" }
  ]
}
